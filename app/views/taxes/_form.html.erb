<%= form_with model: tax do |form|%>
  <div>
    <%= form.label :amount%><br/>
    <%= form.number_field :amount%>
    <% tax.errors.full_messages_for(:amount).each do |message|%>
      <div><%=message%></div>
    <%end%>
  </div>
  <div>
    <%= form.label :description%><br/>
    <%= form.text_field :description%>
    <% tax.errors.full_messages_for(:description).each do |message|%>
      <div><%=message%></div>
    <%end%>
  </div>
  <div>
    <%= form.label :tax_type%><br/>
    <%= form.text_field :tax_type%>
    <% tax.errors.full_messages_for(:tax_type).each do |message|%>
      <div><%=message%></div>
    <%end%>
  </div>
  <div>
    <%= form.label :company_id%><br/>
    <%= form.collection_select :company_id, Company.order(:name), :id, :name%>
    <% tax.errors.full_messages_for(:company_id).each do |message|%>
      <div><%=message%></div>
    <%end%>
  </div>
  <div>
    <%= form.label :due_date%><br/>
    <%= form.date_field :due_date%>
    <% tax.errors.full_messages_for(:due_date).each do |message|%>
      <div><%=message%></div>
    <%end%>
  </div>
  <div>
    <%= form.label :file%><br/>
    <%= form.text_field :file%>
    <% tax.errors.full_messages_for(:file).each do |message|%>
      <div><%=message%></div>
    <%end%>
  </div>
  <div>
    <%= form.label :paid%><br/>
    <%= form.checkbox :paid%>
    <% tax.errors.full_messages_for(:paid).each do |message|%>
      <div><%=message%></div>
    <%end%>
  </div>
  <div>
    <%= form.submit %>
  </div>
<%end%>